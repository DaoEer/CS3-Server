<root>
	<Volatile>
		<position/>
		<!--<position> 0 </position> Don't update-->
		<yaw/>
		<!--<pitch> 20 </pitch>-->
		<pitch/>
		<roll/>
	</Volatile>

	<Implements>
	</Implements>

	<Properties>
		<keyWord> <!-- 激活码 -->
			<Type>				UNICODE			</Type>
			<Flags>				BASE			</Flags>
			<Persistent>		true			</Persistent>
			<DatabaseLength>	128				</DatabaseLength>
			<Index> 			UNIQUE			</Index>
		</keyWord>
		
		<item> <!-- 物品 -->
			<Type>				UNICODE			</Type>
			<Flags>				BASE			</Flags>
			<DatabaseLength>	128				</DatabaseLength>
			<Persistent>		true			</Persistent>
		</item>
		
		<accountDBID> <!-- 激活者账号DBID -->
			<Type>				DBID			</Type>
			<Flags>				BASE			</Flags>
			<Persistent>		true			</Persistent>
		</accountDBID>
		
		<roleName> <!-- 激活者角色名字 -->
			<Type>				UNICODE			</Type>
			<Flags>				BASE			</Flags>
			<DatabaseLength>	128				</DatabaseLength>
			<Persistent>		true			</Persistent>
		</roleName>
		
		<status> <!-- 激活状态：已激活/未激活(1/0) -->
			<Type>				BOOL			</Type>
			<Flags>				BASE			</Flags>
			<Persistent>		true			</Persistent>
			<Default>			0				</Default>
		</status>
	</Properties>

	<ClientMethods>
	</ClientMethods>

	<BaseMethods>
	</BaseMethods>

	<CellMethods>
	</CellMethods>

</root>
